# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google AI Configuration (for Gemini AI matching)
GOOGLE_AI_API_KEY=your_google_ai_api_key
NEXT_PUBLIC_GOOGLE_AI_API_KEY=your_google_ai_api_key

# Neon Database Configuration (alternative to Supabase)
NEON_NEON_DATABASE_URL=postgresql://username:password@host/database
NEON_POSTGRES_URL=postgresql://username:password@host/database
NEON_POSTGRES_PRISMA_URL=postgresql://username:password@host/database?pgbouncer=true&connect_timeout=15
NEON_POSTGRES_URL_NON_POOLING=postgresql://username:password@host/database
NEON_POSTGRES_USER=username
NEON_POSTGRES_HOST=host
NEON_POSTGRES_PASSWORD=password
NEON_POSTGRES_DATABASE=database

# Application Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret

# Optional: Additional AI API Keys
OPENAI_API_KEY=sk-your_openai_api_key
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key

# Development Configuration
NODE_ENV=development
